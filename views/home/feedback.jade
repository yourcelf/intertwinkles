extends ../layout
block body
  h1 Feedback &amp; Support

  div(style='text-align: center;')
    p Thanks for helping to make InterTwinkles better.
    a.btn.btn-primary.btn-large(href="javascript:UserVoice.showPopupWidget();")
      | Send Feedback
  
block postscripts
