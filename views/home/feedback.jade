    intertwinkles.connect_socket(function() {
      intertwinkles.build_toolbar($("header"), {
        applabel: "www",
        active_name: INITIAL_DATA.active_name
      });
      intertwinkles.build_footer($("footer"));
    });
  include includes/uservoice
